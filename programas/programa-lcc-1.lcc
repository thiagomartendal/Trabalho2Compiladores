def fatorial(int x)
{
    int fat;
    for (fat = 1; x > 1; x = x - 1)
    {
        fat = fat * x;
    }
    print "O Fatorial é: ";
    print fat;
    return;
}

def exp(int x)
{
    int result;
    int e;
    e = 2.718281828459045235360287;
    int i;
    result = e;
    for (i = 0; i < x; i = i + 1)
    {
        result = result * e;
    }
    print "O exponencial é: ";
    print e;
    return;
}

def abs(int x)
{
    int result;
    int temp;
    temp = -1;
    result = x;
    if (x < 0)
    {
        result  = x * temp;
    }
    print "O valor absoluto é: ";
    print result;
    return;
}

